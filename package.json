{
  "name": "hawkframework",
  "version": "1.0.11",
  "description": "Free Material Framework with lightweight vue components.",
  "author": "Eduardo Tucci",
  "private": false,
  "main": "./dist/hawkframework.common.js",
  "homepage": "https://github.com/edutucci/hawkframework",
  "keywords": [
    "Vue",
    "Vue Components",
    "Material Design",
    "Vue Material"
  ],
  "license": "MIT",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "scripts": {
    "dev": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "build-lib": "vue-cli-service build --target lib --name hawkframework ./src/index.js",
    "lint": "vue-cli-service lint",
    "copy-unix": "cp ./src/theme/components.styl ./dist/",
    "copy-win": "copy src\\theme\\components.styl dist",
    "build-lib-unix": "yarn build-lib && yarn copy-unix",
    "build-lib-win:": "yarn build-lib && yarn copy-win"
  },
  "dependencies": {
    "inputmask-core": "^2.2.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "parse": "^2.0.0",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "register-service-worker": "^1.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "uuid": "^3.3.2",
    "vue": "^2.5.16",
    "vue-awesome-swiper": "^3.1.3",
    "vue-clickaway": "^2.2.2",
    "vue-focus": "^2.1.0",
    "vue-highlightjs": "^1.3.3",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.10",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.10",
    "@vue/cli-plugin-pwa": "^3.0.0-rc.10",
    "@vue/cli-service": "^3.0.0-rc.10",
    "@vue/eslint-config-standard": "^3.0.0-rc.10",
    "vue-template-compiler": "^2.5.16"
  }
}
